#! /usr/bin/gnuplot -persist
set xlabel "X" 
set ylabel "Y"
set grid
set yrange [-4:4]
set xrange[-5:15]
set title "Method graph" font "Helvetica Bold, 20"
set logscale xy
 set logscale y 10
 set logscale x 10
plot "x_pogr.txt" with lines lw 3, "s_pogr.txt" with lines lw 3, "step_pogr.txt" with lines lw 3 ti "y"
pause -1